#undef  WIN32_LEAN_AND_MEAN
#define WIN32_LEAN_AND_MEAN
#include <windows.h>

VS_VERSION_INFO VERSIONINFO
FILEVERSION ${CU_PROJECT_VERSION_MAJOR},${CU_PROJECT_VERSION_MINOR},${CU_PROJECT_VERSION_PATCH},${CU_PROJECT_VERSION_BETA}
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "0409FDE9"
    BEGIN
			VALUE "CompanyName", "${CU_PROJECT_COMPANYNAME}\0"
			VALUE "FileDescription", "${CU_PROJECT_PRODUCTDESCRIPTION}\0"
			VALUE "FileVersion", "${CU_PROJECT_FILEVERSION_STRING}\0"
			VALUE "ProductName", "${PROJECT_NAME}\0"
			VALUE "ProductVersion", "${CU_PROJECT_FRIENDLY_VERSION}\0"
			VALUE "LegalCopyright", "${CU_PROJECT_LEGALCOPYRIGHT}\0"
    END
  END

  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x409, 65001
  END
END

IDI_ICON1 ICON "${CU_ROOT_DIR}/resources/win32/Icon.ico"
